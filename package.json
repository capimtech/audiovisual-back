    "date-fns": "^3.3.1",
    "dotenv": "^16.4.5",
    "google-auth-library": "^9.6.3",
    "helmet": "^7.1.0",
    "iconv-lite": "^0.6.3",
    "pg": "^8.11.3",
    "reflect-metadata": "^0.2.1",
    "rxjs": "^7.8.1",
    "typeorm": "^0.3.20",
    "zod": "^3.22.4"
